int gcd(int a, int b) {
  return if(
    () -> b.isZero(),
    () -> a,
    () -> gcd(b, a.mod(b))
  )
}

int gcd2(int a, int b) {
  while(
    () -> b.isZero().not(),
    () -> {
        set(a, a.mod(b))
       swap(a,b)
    }
  )
  return a;
}

int whileTest(){
    while(
        () -> i.isZero().not(),
        () -> {
            set(i, i.add(one()))
        }
    )
}

